{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "#!\/bin\/zsh\njekyll build "
    },
    "run" : {
      "enabled" : true,
      "script" : "#!\/bin\/zsh\njekyll serve --trace -w --future --livereload --force_polling --config _config.yml,_localhost.yml"
    }
  },
  "openLogOnRun" : "start",
  "persistent" : true
}
