# Vieux liens avant que j'utilise /articles
RedirectMatch 301 (.*)/1989/(.*) $1/articles/$2
RedirectMatch 301 (.*)/1990/(.*) $1/articles/$2
RedirectMatch 301 (.*)/1991/(.*) $1/articles/$2
RedirectMatch 301 (.*)/1992/(.*) $1/articles/$2
RedirectMatch 301 (.*)/1993/(.*) $1/articles/$2
RedirectMatch 301 (.*)/1995/(.*) $1/articles/$2
RedirectMatch 301 (.*)/1998/(.*) $1/articles/$2
RedirectMatch 301 (.*)/1999/(.*) $1/articles/$2
RedirectMatch 301 (.*)/2000/(.*) $1/articles/$2
RedirectMatch 301 (.*)/2001/(.*) $1/articles/$2

# J'ai chang√© le nom du fichier timeline pour timelines
RedirectMatch 301 (.*)/timeline/(.*) $1/timelines/$2

# Fichier de la page d'accueil
Redirect 301 /main.html /

RewriteEngine on
RewriteBase /
RewriteRule ^(.+)/$  /$1 [R=301,L]
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule !.*\.html$ %{REQUEST_FILENAME}.html [L]

RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteRule ^ /%1 [NC,L,R]

RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^ %{REQUEST_URI}.html [NC,L]

<ifModule headers_module>
    <FilesMatch ".(ico|pdf|jpg|jpeg|png|gif|mp4|mp3|svg)$">
      Header set Cache-Control "max-age=36000, public"
    </FilesMatch>
    <FilesMatch ".(css|webmanifest)$">
      Header set Cache-Control "max-age=3600, public"
    </FilesMatch>
</ifModule>