---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for collection in site.collections %}
  {% for page in site[collection.label] %}
    <url>
      <loc>{{ site.url }}{{ page.url }}</loc>
      <lastmod>{{ page.last_modified_at | date: '%Y-%m-%d' }}</lastmod>
    </url>
  {% endfor %}
{% endfor %}

{% comment %} If timelines are skipped, this is inserted {% endcomment %}
{% unless site.collections contains timelines %}<url><loc>https://gameboyessentials.com/timelines/eu</loc><lastmod>2026-01-07</lastmod></url><url><loc>https://gameboyessentials.com/timelines/jp</loc><lastmod>2026-01-07</lastmod></url><url><loc>https://gameboyessentials.com/timelines/na</loc><lastmod>2026-01-07</lastmod></url>{% endunless %}

{% assign pdf-list = site.static_files | where: "extname", ".pdf" %}
{% for object in pdf-list %}
  {% if object.extname == ".pdf" %}
    <url>
      <loc>{{ site.url }}{{ object.path }}</loc>
      <lastmod>{{ object.modified_time | date: '%Y-%m-%d' }}</lastmod>
    </url>
  {% endif %}
{% endfor %}
</urlset>